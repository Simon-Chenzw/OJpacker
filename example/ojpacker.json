{
    "defalut_zip_name": "problem_data",
    "state_name": "state",
    "input_data_name": "data{num}.in",
    "output_data_name": "data{num}.out",
    "input_default_exec": "py",
    "output_default_exec": "cpp",
    "input_exec": {
        "cpp": {
            "src": "make_in.cpp",
            "exe": "make_in.out",
            "compile_cmd": "g++ {src} -o {exe}",
            "execute_cmd": "./{exe}"
        },
        "py": {
            "src": "make_in.py",
            "execute_cmd": "python3 {src}"
        }
    },
    "output_exec": {
        "cpp": {
            "src": "make_out.cpp",
            "exe": "make_out.out",
            "compile_cmd": "g++ {src} -o {exe}",
            "execute_cmd": "./{exe}"
        },
        "py": {
            "src": "make_out.py",
            "execute_cmd": "python3 {src}"
        }
    }
}